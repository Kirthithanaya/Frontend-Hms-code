{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" }
  ],
  "redirects": [
    { "source": "/old-page", "destination": "/new-page", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Custom-Header", "value": "my value" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
